      <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_-8mmkDB7EeyV-cxGvWHLLw" name="main" specification="_-7yHMDB7EeyV-cxGvWHLLw">
        <ownedParameter xmi:id="_-8mmkTB7EeyV-cxGvWHLLw" name="args">
