Algoritmo "semnome"

Var

sal, fin:real

Inicio
escreva("Digite o valor do seu salario: ")
     leia(sal)

escreva("Digite o valor do financiamento: ")
     leia(fin)

se (fin <= (sal*5)) entao
     escreval("Parabens! Seu financiamento foi aprovado")
senao
     escreval("Infelizmente nao foi dessa vez.")
fimse

escreval("Obrigado por nos consultar.")

Fimalgoritmo